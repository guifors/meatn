<!-- First carousel -->
<div class="explore-events-text">
  <h5 class="pb-3 border-bottom">Find tables to join today</h5>
</div>
  <div id="carouselExampleControls" class="carousel slide">
    <div class="carousel slide">
      <div class="carousel-inner">
        <div class="carousel-item active">
          <div class="row">
            <% @bookings.first(5).drop(2).each do |booking| %>
              <div class="col-sm">
                <div class="card-trip">
                  <img class="carousel-image" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url('<%= booking.restaurant.image_url %>')" />
                  <div class="card-trip-infos">
                    <div>
                      <h2><%= booking.restaurant.name %></h2>
                      <p><%= booking.restaurant.food_type %>
                      <% if booking.restaurant.price == 1 %>
                         · $</p>
                      <% elsif booking.restaurant.price == 2 %>
                        · $$</p>
                      <% else %>
                        · $$$</p>
                      <% end %>
                        <p><%= booking.restaurant.short_address %></p>
                </div>
                <div class="d-flex justify-content-between">
                      <p>
                        <% blank_stars = 5 - booking.restaurant.user_rating %>
                          <% booking.restaurant.user_rating.times do  %>
                          <i class="fas fa-star filledstars"></i>
                          <% end %>
                          <% blank_stars.times do %>
                          <i class="far fa-star blankstars"></i>
                          <% end %>
                          (<%= booking.restaurant.user_reviews %>)
                      </p>
                    <div class="all-cards-avatar">
                        <% if booking.user.photo.attached? %>
                          <%= cl_image_tag booking.user.photo.key, class: "card-trip-user avatar-bordered-card avatar-image", id: "navbarDropdown", data: { toggle: "dropdown" }, 'aria-haspopup': true, 'aria-expanded': false %>
                        <% else %>
                          <%= image_tag "default_user.png", class: "card-trip-user avatar-bordered-card avatar-image", id: "navbarDropdown", data: { toggle: "dropdown" }, 'aria-haspopup': true, 'aria-expanded': false %>
                        <% end %>
                        <% booking.users.each do |user| %>
                          <% if user.photo.attached? %>
                            <%= cl_image_tag user.photo.key, class: "card-trip-user avatar-bordered-card avatar-image", id: "navbarDropdown", data: { toggle: "dropdown" }, 'aria-haspopup': true, 'aria-expanded': false %>
                          <% else %>
                            <%= image_tag "default_user.png", class: "card-trip-user avatar-bordered-card avatar-image", id: "navbarDropdown", data: { toggle: "dropdown" }, 'aria-haspopup': true, 'aria-expanded': false %>
                          <% end %>
                        <% end %>
                     </div>
                </div>
                  </div>
                </div>
              </div>
            <% end %>
          </div>
        </div>
        <div class="carousel-item">
          <div class="row" >

            <% @bookings.first(8).drop(5).each do |booking| %>
              <div class="col-sm">
                <div class="card-trip">
